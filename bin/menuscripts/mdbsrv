#!/bin/sh

services() {
    cat <<-EOF
    5000 dufs
    8096 jellyfin
    24961 miniflux
    80 ntfy
    24880 pihole
    5030 slsksd
    8384 syncthing
    9091 transmission
EOF
}

service="$(services | column -t -l 2 | dmenu -c -x)"
[ "$service" ] || exit 1
$BROWSER "http://192.168.178.79:${service%% *}"
