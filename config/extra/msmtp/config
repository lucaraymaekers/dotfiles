account billy@keyfried.com
host mail.keyfried.com
port 465
from billy@keyfried.com
user billy@keyfried.com
passwordeval "pass hosts/spacehb/billy"
auth on
tls on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile /home/aluc/.local/state/msmtp/msmtp.log
tls_starttls off

